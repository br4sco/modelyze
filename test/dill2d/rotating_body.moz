include DILLEXPORT
include DILLSIM

def n_1, n_2: Node

def t: Real
def t_0 = 0.

def m = 1.
def I = 1.
def x_m, y_m: Real
def vx_m, vy_m: Real
def Fx_m, Fy_m: Real
def th_m, om_m, T_m: Real
def rr_m = vec x_m y_m
def vv_m = vec vx_m vy_m
def FF_m = vec Fx_m Fy_m

def b = Body m
             I
             rr_m
             vv_m
             FF_m
             th_m
             om_m
             T_m
             n_1 n_2

def x_h, y_h: Real
def Fx_h, Fy_h: Real
def th_h, om_h, T_h: Real
def rr_h = vec x_h y_h
def FF_h = vec Fx_h Fy_h
def f = t * t
def dft = 2. * t

def aa = AngularActuator f
                         dft
                         rr_h
                         FF_h
                         th_h
                         om_h
                         T_h
                         n_1 n_2

def c = Clock t_0 t
def m = b; aa; c

def main = {
  // exportDill2Modelica "Rotating_Body" m
  simulate m (0., 10.) 0.01
}
