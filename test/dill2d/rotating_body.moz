include DILLEXPORT

def n_1, n_2: Node

def t: Real
def t_0 = 0.

def m = 1.
def I = 1.
def x_m, y_m: Real
def vx_m, vy_m: Real
def ax_m, ay_m: Real
def Fx_m, Fy_m: Real
def th_m, om_m, al_m, T_m: Real
def rr_m = vec x_m y_m
def vv_m = vec vx_m vy_m
// def aa_m = vec ax_m ay_m
def FF_m = vec Fx_m Fy_m

def b = Body m
             I
             rr_m
             vv_m
             // aa_m
             FF_m
             th_m
             om_m
             // al_m
             T_m
             n_1 n_2

def x_h, y_h: Real
def vx_h, vy_h: Real
def ax_h, ay_h: Real
def Fx_h, Fy_h: Real
def th_h, om_h, al_h, T_h: Real
def rr_h = vec x_h y_h
def vv_h = vec vx_h vy_h
// def aa_h = vec ax_h ay_h
def FF_h = vec Fx_h Fy_h
def f = t * t
def dft = 2. * t
// def dftt = 2.

def aa = AngularActuator f
                         dft
                         // dftt
                         rr_h
                         vv_h
                         // aa_h
                         FF_h
                         th_h
                         om_h
                         // al_h
                         T_h
                         n_1 n_2

def c = Clock t_0 t
def m = b; aa; c

def main = {
  exportDill2Modelica "Rotating_Body" m
}
