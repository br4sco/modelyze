def myRJoint = RevoluteJoint1 DATUM 0.2

def model = Union [
                    myRJoint DATUM f_1
                    , myBar 2. 0.2 1. f_1 f_2
                    , myRJoint f_2 f_3
                    , myBar 2. 0.2 1. f_3 f_4
                    , myRJoint f_4 f_5
                    , myBar 2. 0.2 1. f_5 f_6
                  ]

def main = {
  elaborateToFile model
}
