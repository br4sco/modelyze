include Matrix
include Equation

def r_1, r_2, r_3: Real

def I = mmSRealId 3
def M = mmSRealZero (3, 3)
def A = listToMatrix [[r_1, r_2, r_3],
                      [r_1 + r_2, r_2 + r_3, r_1 + r_3],
                      [r_1, r_2, r_3]
                     ] M

def C = listToMatrix [[r_1 - r_3],
                      [r_2 - r_3],
                      [r_1 + r_3]
                     ] (mmSRealZero (3, 1))

def f(v: <Real>) = { der v }
def ff(v: <Real>) = { sin v }

def _ = dp (map f A)
def _ = dp (map ff C)
