include Matrix

def M = mmRealZero (3, 3)
def A = listToMatrix [[0., 0., 0.], [1., 2., 3.], [0., 0., 0.]] M
def B = listToMatrix [[1., 2., 3.], [4., 5., 6.], [7., 8., 9.]] M
def C = listToMatrix [[4., 5., 6.], [1., 2., 3.], [7., 8., 9.]] M
def D = listToMatrix [[7., 8., 9.], [4., 5., 6.], [1., 2., 3.]] M

def _ = dprint(rowopj (fun j: Int -> int2real j) 2 M == A)
def _ = dprint(swrows 1 2 B == C)
def _ = dprint(swrows 1 3 B == D)
def _ = dprint(swrows 1 1 A == A)
