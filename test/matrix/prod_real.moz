include Matrix

def M = mmRealZero (3, 3)
def I = mmRealId 3
def A = listToMatrix [[1., 2., 3.], [4., 5., 6.], [7., 8., 9.]] M
def B = listToMatrix [[0., 1., 2.], [3., 4., 5.], [6., 7., 8.]] M
def C = listToMatrix [[24., 30., 36.], [51., 66., 81.], [78., 102., 126.]] M
def v = listToMatrix [[1.], [2.], [3.]] (mmRealZero (3, 1))
def u = listToMatrix [[1., 2., 3.]] (mmRealZero (1, 3))
def D = listToMatrix [[24., 30., 36.], [51., 66., 81.], [78., 102., 126.]] M

def _ = dprintln(A * I == A)
def _ = dprintln(A * B == C)
def _ = dprintln(I * v == v)
def _ = dprintln(u * I == u)
