include Matrix

def M = mmRealZero (3, 3)
def I = mmRealId 3
def A = listToMatrix [[1., 2., 3.], [4., 5., 6.], [7., 8., 9.]] M
def At = listToMatrix [[1., 4., 7.], [2., 5., 8.], [3., 6., 9.]] M
def v = listToMatrix [[1.], [2.], [3.]] (mmRealZero (3, 1))
def vt = listToMatrix [[1., 2., 3.]] (mmRealZero (1, 3))

def _ = dprintln(tr I == I)
def _ = dprintln(tr A == At)
def _ = dprintln(tr At == A)
def _ = dprintln(tr v == vt)
def _ = dprintln(tr vt == v)
