include Matrix

def M = matrixZero 0 (3, 3)

def _ = dprint((get (1, 1) M) == 0)
def _ = dprint((get (2, 1) M) == 0)
def _ = dprint((get (3, 3) M) == 0)

def I = (set (3, 3) 1 (set (2, 2) 1 (set (1, 1) 1 M)))

def _ = dprint((get (1, 1) I) == 1)
def _ = dprint((get (2, 2) I) == 1)
def _ = dprint((get (3, 3) I) == 1)

def _ = dprint((get (1, 2) I) == 0)
def _ = dprint((get (3, 2) I) == 0)
