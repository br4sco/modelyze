include Matrix

def M = matrixMapIntZero (3, 3)
def I = matrixMapIntId 3
def A = listToMatrix [[1, 2, 3], [4, 5, 6], [7, 8, 9]] M
def At = listToMatrix [[1, 4, 7], [2, 5, 8], [3, 6, 9]] M
def v = listToMatrix [[1], [2], [3]] (matrixMapIntZero (3, 1))
def vt = listToMatrix [[1, 2, 3]] (matrixMapIntZero (1, 3))

def _ = dprint(tr I == I)
def _ = dprint(tr A == At)
def _ = dprint(tr At == A)
def _ = dprint(tr v == vt)
def _ = dprint(tr vt == v)
