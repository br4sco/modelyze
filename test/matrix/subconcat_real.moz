include Matrix

def M = mmRealZero (3, 3)
def I_3 = mmRealId 3
def I_1 = mmRealId 1
def A = listToMatrix [[1., 2., 3.], [4., 5., 6.], [7., 8., 9.]] M
def v = listToMatrix [[1.], [2.], [3.]] (mmRealZero (3, 1))
def us = listToMatrix [[2., 3.]] (mmRealZero (1, 2))
def Bh = listToMatrix [[1., 2.], [3., 4.]] (mmRealZero (2, 2))
def Bt = listToMatrix [[5., 6.], [7., 8.], [9., 10.]] (mmRealZero (3, 2))

def _ = dp (sub (1, 1) (2, 2) I_3)
def _ = dp (sub (1, 1) (2, 2) A)
def _ = dp (sub (2, 1) (3, 1) v)
def _ = dp (sub (1, 1) (1, 3) A)

def _ = dp (vconcat I_1 us)
def _ = dp (vconcat [I_1, us])
def _ = dp (hconcat Bh Bt)
