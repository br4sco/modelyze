include Graph

def n1, n2, n3, n4, n5: Node
def G = listToMap [(n1, [n2, n3]), (n2, [n3, n1]), (n3, [n2, n3])]
def p1 = [[n1]]
def p2 = [[n1, n2]]
def p3 = [[n1, n3]]

def _ = dprintln(seq (findPath G n1 n1) p1)
def _ = dprintln(seq (findPath G n1 n2) p2)
def _ = dprintln(seq (findPath G n1 n3) p3)

def G2 = listToMap [(n1, [n2, n4]), (n2, [n3, n1]), (n3, [n2, n4]), (n4, [n1, n3])]
def p4 = [[n1]]
def p5 = [[n1, n2]]
def p6 = [[n1, n2, n3],[n1, n4, n3]]

def _ = dprintln(seq (findPath G2 n1 n1) p4)
def _ = dprintln(seq (findPath G2 n1 n2) p5)
def _ = dprintln(seq (findPath G2 n1 n3) p6)

def G3 = listToMap [(n1, [n2]), (n2, [n1, n3, n4, n5]), (n3, [n2]), (n4, [n2]), (n5, [n2])]
def p7 = [[n1]]
def p8 = [[n1, n2]]
def p9 = [[n1, n2, n3]]
def p10 = [[n4, n2, n3]]

def _ = dprintln(seq (findPath G3 n1 n1) p7)
def _ = dprintln(seq (findPath G3 n1 n2) p8)
def _ = dprintln(seq (findPath G3 n1 n3) p9)
def _ = dprintln(seq (findPath G3 n4 n3) p10)
