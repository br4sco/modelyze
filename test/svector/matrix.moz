include Vector

def v_1, v_2, v_3: Vector

def M = mmVecZero (3, 3)
def I = mmRealId 3
def A = listToMatrix [[v_1, v_2, v_3],
                      [v_1 + v_2, v_2 + v_3, v_1 + v_3],
                      [v_1, v_2, v_3]
                     ] M

def v = listToMatrix [[v_1, v_2, v_3]] (mmVecZero (1, 3))
def B = listToMatrix [[1., 0., -1.],
                      [0., 1., -1.],
                      [1., 0., 1.]
                     ] (mmRealZero (3, 3))
def C = listToMatrix [[v_1 - v_3],
                      [v_2 - v_3],
                      [v_1 + v_3]
                     ] (mmVecZero (3, 1))

def _ = dp M
def _ = dp (I * M)

def _ = dp A
def _ = dp (I * A)

def _ = dp C
def _ = dp (B * (tr v))
