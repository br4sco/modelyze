include GRAPHSIMELECTROMECHANICAL

def node1, node2: Node
def frame1, frame2, frame3: Frame
def testEdge: Frame -> Frame -> Real -> Real -> Model

def l = fun f1: Frame -> fun f2: Frame ->
          def lx: Real;
          def lF: Real;
          testEdge f1 f2 lx lF

def r = fun f1: Frame -> fun f2: Frame ->
          def rx: Real;
          def rF: Real;
          testEdge f1 f2 rx rF

def ll = fun f1: Frame -> fun f2: Frame -> fun n1: Node -> fun n2: Node ->
          def lx: Real;
          def lF: Real;
          def lu: Voltage;
          def li: Current;
          testEdge f1 f2 lx lF;
          electricalEdge n1 n2 lu li

def rr = fun f1: Frame -> fun f2: Frame -> fun n1: Node -> fun n2: Node ->
          def rx: Real;
          def rF: Real;
          def ru: Voltage;
          def ri: Current;
          testEdge f1 f2 rx rF;
          electricalEdge n1 n2 ru ri

def _ = print("1: ")
def _ = dprintln (frame1 -- l -- frame2)

def _ = print("2: ")
def _ = dprintln (frame1 -- l -- r -- frame2)

def _ = print("3: ")
def _ = dprintln (frame1 -- (l || r) -- frame2)

def _ = print("4: ")
def _ = dprintln (frame1 -- l -.- frame3 -.- r -- frame2)

def _ = print("5: ")
def _ = dprintln (node1 -- (frame1 -- ll -- frame2) -- node2)

def _ = print("6: ")
def _ = dprintln (node1 -- (frame1 -- ll -- rr -- frame2) -- node2)

def _ = print("7: ")
def _ = dprintln (node1 -- (frame1 -- (ll || rr) -- frame2) -- node2)

def _ = print("8: ")
def _ = dprintln (node1 -- (frame1 -- ll -- r -- frame2) -- node2)

def _ = print("9: ")
def _ = dprintln (node1 -- (frame1 -- l -- rr -- frame2) -- node2)

def _ = print("10: ")
def _ = dprintln (node1 -- (frame1 -- (ll || r) -- frame2) -- node2)

def _ = print("11: ")
def _ = dprintln (node1 -- (frame1 -- (l || rr) -- frame2) -- node2)

def _ = print("12: ")
def _ = dprintln (node1 -- (frame1 -- (ll -- rr || r) -- frame2) -- node2)

def _ = print("13: ")
def _ = dprintln (node1 -- (frame1 -- (ll -- r || r) -- frame2) -- node2)

def _ = print("13: ")
def _ = dprintln (node1 -- (frame1 -- (l -- rr || r) -- frame2) -- node2)
