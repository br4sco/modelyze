include EquationsFormulation

def x_1, x_2, x_3: Vector
def y_1, y_2, y_3: Vector
def r_1, r_2, r_3, r_4: Real
def F_1, F_2, F_3, F_4: Real

def f(v: MatrixMapVector) -> [[Vector]] = { matrixToList v }
def ff(v: MatrixMapSReal) -> [[<Real>]] = { matrixToList v }

def IM1 = listToMatrix [
                        [ 1.,  0., -1.],
                        [-1.,  1.,  0.],
                        [ 0., -1.,  1.]
                      ] (mmRealZero (3, 3))

def X1 = [
            listToMatrix [[x_1], [x_2], [x_3]] (mmVecZero (3, 1)),
            listToMatrix [[y_1], [y_2], [y_3]] (mmVecZero (3, 1))
          ]

def Y1 = [
            listToMatrix [[x_1], [x_2], [x_3]] (mmVecZero (3, 1)),
            listToMatrix [[y_1], [y_2], [y_3]] (mmVecZero (3, 1))
          ]

def A1r = listToMatrix [
                        [ -1.],
                        [ -1.]
                      ] (mmRealZero (2, 1))
def X1r = [
            [[x_1], [x_2], [x_3]],
            [[y_1], [y_2], [y_3]]
           ]

def Y1r = [
            [[x_1], [x_2], [x_3]],
            [[y_1], [y_2], [y_3]]
           ]

def IM2 = listToMatrix [[0., 0., 1., 1.], [1., -1., -1., 0.], [-1., 1., 0., -1.]] (mmRealZero (3, 4))
def X2 = [
            listToMatrix [[r_1], [r_2], [r_3], [r_4]] (mmSRealZero (4, 1)),
            listToMatrix [[F_1], [F_2], [F_3], [F_4]] (mmSRealZero (4, 1))
          ]

def Y2 = [
            listToMatrix [[r_1], [r_2], [r_3], [r_4]] (mmSRealZero (4, 1)),
            listToMatrix [[F_1], [F_2], [F_3], [F_4]] (mmSRealZero (4, 1))
          ]

def A2r = listToMatrix [
                        [-1., 1.],
                        [ 0., 1.]
                       ] (mmRealZero (2, 2))

def X2r = [
            [[r_1], [r_3], [r_2], [r_4]],
            [[F_1], [F_3], [F_2], [F_4]]
           ]

def Y2r = [
            [[r_1], [r_3], [r_2], [r_4]],
            [[F_1], [F_3], [F_2], [F_4]]
           ]

def main = {
  def (A1, nX1, nY1) = mkCutsetMatrix IM1 X1 Y1;
  def _ = dprint(A1 == A1r);
  def _ = dprint(seq (map f nX1) X1r);
  def _ = dprint(seq (map f nY1) Y1r);

  def (A2, nX2, nY2) = mkCutsetMatrix IM2 X2 Y2;
  def _ = dprint(A2 == A2r);
  def _ = dprint(seq (map ff nX2) X2r);
  def _ = dprint(seq (map ff nY2) Y2r);
  ()
}
