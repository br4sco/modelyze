include ModelyzeHA

def test() = {
    def x, dx, y: Real;
    init x 0.;
    init y 5.;
    init dx 2.;
    predicate (x > 2.);
    predicate (y < 0.);
    y'' = -1.;
    x'' = 0.;
    x' = dx;
    probe "x" = x;
    probe "y" = y
}

def main = {
    def model = test();
    printsim model 0.1 10.
}


